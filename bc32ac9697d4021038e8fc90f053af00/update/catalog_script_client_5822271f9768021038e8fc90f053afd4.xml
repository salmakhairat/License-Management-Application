        <script><![CDATA[function onSubmit() {
    //Type appropriate comment here, and begin script below
    var dateOfBirthField = g_form.getValue('x_1318047_license_dob'); // Assuming 'date_of_birth' is the field name
    var dateOfBirth = new Date(dateOfBirthField);
    var sixteenYearsAgo = new Date();
    sixteenYearsAgo.setFullYear(sixteenYearsAgo.getFullYear() - 16);

    if (dateOfBirth <= sixteenYearsAgo) {
        // Date of birth is 16 years old or more
        return true; // Proceed with form submission
    } else {
        // Date of birth is less than 18 years old
        alert('You must be 16 years old or older to proceed.');
        return false; // Prevent form submission
    }
}]]></script>
