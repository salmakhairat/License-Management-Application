        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
    // Get the person's date of birth from the record
    var dob = new GlideDateTime(current.x_1318047_license_dob); // Assuming 'date_of_birth' is the field name

    // Get the current date
    var currentDate = new GlideDateTime();

    // Calculate the difference in years between the current date and the date of birth
    var yearsDiff = GlideDateTime.subtract(currentDate, dob);
    yearsDiff = yearsDiff.getNumericValue() / 1000 / 60 / 60 / 24 / 365.25; // Convert milliseconds to years

    // Check if the person is over 18 years old
    if (yearsDiff >= 18) {
        // Set the field value accordingly
        current.is_over_18 = 'Yes'; // Assuming 'is_over_18' is a Boolean field
    } else {
        current.is_over_18 = 'No';
    }
})(current, previous);]]></script>
